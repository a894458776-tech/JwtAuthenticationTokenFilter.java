<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kylin.kton.system.mapper.KtonIpDynamicsAccountAllowIpMapper">
    
    <resultMap type="KtonIpDynamicsAccountAllowIp" id="KtonIpDynamicsAccountAllowIpResult">
        <result property="id"    column="id"    />
        <result property="merchantId"    column="merchant_id"    />
        <result property="accountId"    column="account_id"    />
        <result property="ipAddress"    column="ip_address"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateTime"    column="update_time"    />
    </resultMap>

    <sql id="selectKtonIpDynamicsAccountAllowIpVo">
        select id, merchant_id, account_id, ip_address, create_time, update_time from kton_ip_dynamics_account_allow_ip
    </sql>

    <select id="selectKtonIpDynamicsAccountAllowIpList" parameterType="KtonIpDynamicsAccountAllowIp" resultMap="KtonIpDynamicsAccountAllowIpResult">
        <include refid="selectKtonIpDynamicsAccountAllowIpVo"/>
        <where>  
            <if test="merchantId != null "> and merchant_id = #{merchantId}</if>
            <if test="accountId != null "> and account_id = #{accountId}</if>
            <if test="ipAddress != null  and ipAddress != ''"> and ip_address = #{ipAddress}</if>
        </where>
    </select>
    
    <select id="selectKtonIpDynamicsAccountAllowIpById" parameterType="Long" resultMap="KtonIpDynamicsAccountAllowIpResult">
        <include refid="selectKtonIpDynamicsAccountAllowIpVo"/>
        where id = #{id}
    </select>

    <insert id="insertKtonIpDynamicsAccountAllowIp" parameterType="KtonIpDynamicsAccountAllowIp" useGeneratedKeys="true" keyProperty="id">
        insert into kton_ip_dynamics_account_allow_ip
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="merchantId != null">merchant_id,</if>
            <if test="accountId != null">account_id,</if>
            <if test="ipAddress != null and ipAddress != ''">ip_address,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateTime != null">update_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="merchantId != null">#{merchantId},</if>
            <if test="accountId != null">#{accountId},</if>
            <if test="ipAddress != null and ipAddress != ''">#{ipAddress},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateTime != null">#{updateTime},</if>
         </trim>
    </insert>

    <update id="updateKtonIpDynamicsAccountAllowIp" parameterType="KtonIpDynamicsAccountAllowIp">
        update kton_ip_dynamics_account_allow_ip
        <trim prefix="SET" suffixOverrides=",">
            <if test="merchantId != null">merchant_id = #{merchantId},</if>
            <if test="accountId != null">account_id = #{accountId},</if>
            <if test="ipAddress != null and ipAddress != ''">ip_address = #{ipAddress},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteKtonIpDynamicsAccountAllowIpById" parameterType="Long">
        delete from kton_ip_dynamics_account_allow_ip where id = #{id}
    </delete>

    <delete id="deleteKtonIpDynamicsAccountAllowIpByIds" parameterType="String">
        delete from kton_ip_dynamics_account_allow_ip where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>