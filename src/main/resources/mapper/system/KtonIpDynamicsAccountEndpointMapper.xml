    <sql id="selectKtonIpDynamicsAccountEndpointVo">
        select id, merchant_id, account_id, endpoint_host, endpoint_port, endpoint_status, expire_time, use_traffic_gb, limit_traffic_gb, supported_protocol, remarks, create_time, update_time from kton_ip_dynamics_account_endpoint
