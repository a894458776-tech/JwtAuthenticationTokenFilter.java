    <select id="selectKtonIpDynamicsAccountEndpointList" parameterType="KtonIpDynamicsAccountEndpoint" resultMap="KtonIpDynamicsAccountEndpointResult">
        <include refid="selectKtonIpDynamicsAccountEndpointVo"/>
        <where>  
            <if test="merchantId != null "> and merchant_id = #{merchantId}</if>
            <if test="accountId != null "> and account_id = #{accountId}</if>
            <if test="endpointHost != null  and endpointHost != ''"> and endpoint_host = #{endpointHost}</if>
            <if test="endpointPort != null "> and endpoint_port = #{endpointPort}</if>
            <if test="endpointStatus != null "> and endpoint_status = #{endpointStatus}</if>
            <if test="expireTime != null "> and expire_time = #{expireTime}</if>
            <if test="useTrafficGb != null "> and use_traffic_gb = #{useTrafficGb}</if>
            <if test="limitTrafficGb != null "> and limit_traffic_gb = #{limitTrafficGb}</if>
            <if test="supportedProtocol != null  and supportedProtocol != ''"> and supported_protocol = #{supportedProtocol}</if>
            <if test="remarks != null  and remarks != ''"> and remarks = #{remarks}</if>
        </where>
    </select>
    
    <select id="selectKtonIpDynamicsAccountEndpointById" parameterType="Long" resultMap="KtonIpDynamicsAccountEndpointResult">
        <include refid="selectKtonIpDynamicsAccountEndpointVo"/>
