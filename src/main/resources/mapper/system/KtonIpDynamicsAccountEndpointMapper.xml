    <delete id="deleteKtonIpDynamicsAccountEndpointById" parameterType="Long">
        delete from kton_ip_dynamics_account_endpoint where id = #{id}
    </delete>

    <delete id="deleteKtonIpDynamicsAccountEndpointByIds" parameterType="String">
        delete from kton_ip_dynamics_account_endpoint where id in 
