<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kylin.kton.system.mapper.KtonIpDynamicsChannelItemMapper">
    
    <resultMap type="KtonIpDynamicsChannelItem" id="KtonIpDynamicsChannelItemResult">
        <result property="id"    column="id"    />
        <result property="merchantId"    column="merchant_id"    />
        <result property="channelStatus"    column="channel_status"    />
        <result property="channelName"    column="channel_name"    />
        <result property="channelPassword"    column="channel_password"    />
        <result property="useTrafficGb"    column="use_traffic_gb"    />
        <result property="limitTrafficGb"    column="limit_traffic_gb"    />
        <result property="remarks"    column="remarks"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateTime"    column="update_time"    />
    </resultMap>

    <sql id="selectKtonIpDynamicsChannelItemVo">
        select id, merchant_id, channel_status, channel_name, channel_password, use_traffic_gb, limit_traffic_gb, remarks, create_time, update_time from kton_ip_dynamics_channel_item
    </sql>

    <select id="selectKtonIpDynamicsChannelItemList" parameterType="KtonIpDynamicsChannelItem" resultMap="KtonIpDynamicsChannelItemResult">
        <include refid="selectKtonIpDynamicsChannelItemVo"/>
        <where>  
            <if test="channelStatus != null "> and channel_status = #{channelStatus}</if>
            <if test="channelName != null  and channelName != ''"> and channel_name like concat('%', #{channelName}, '%')</if>
            <if test="channelPassword != null  and channelPassword != ''"> and channel_password = #{channelPassword}</if>
            <if test="useTrafficGb != null "> and use_traffic_gb = #{useTrafficGb}</if>
            <if test="limitTrafficGb != null "> and limit_traffic_gb = #{limitTrafficGb}</if>
            <if test="remarks != null  and remarks != ''"> and remarks = #{remarks}</if>
        </where>
    </select>
    
    <select id="selectKtonIpDynamicsChannelItemById" parameterType="Long" resultMap="KtonIpDynamicsChannelItemResult">
        <include refid="selectKtonIpDynamicsChannelItemVo"/>
        where id = #{id}
    </select>

    <insert id="insertKtonIpDynamicsChannelItem" parameterType="KtonIpDynamicsChannelItem">
        insert into kton_ip_dynamics_channel_item
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">id,</if>
            <if test="merchantId != null">merchant_id,</if>
            <if test="channelStatus != null">channel_status,</if>
            <if test="channelName != null">channel_name,</if>
            <if test="channelPassword != null">channel_password,</if>
            <if test="useTrafficGb != null">use_traffic_gb,</if>
            <if test="limitTrafficGb != null">limit_traffic_gb,</if>
            <if test="remarks != null">remarks,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateTime != null">update_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null">#{id},</if>
            <if test="merchantId != null">#{merchantId},</if>
            <if test="channelStatus != null">#{channelStatus},</if>
            <if test="channelName != null">#{channelName},</if>
            <if test="channelPassword != null">#{channelPassword},</if>
            <if test="useTrafficGb != null">#{useTrafficGb},</if>
            <if test="limitTrafficGb != null">#{limitTrafficGb},</if>
            <if test="remarks != null">#{remarks},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateTime != null">#{updateTime},</if>
         </trim>
    </insert>

    <update id="updateKtonIpDynamicsChannelItem" parameterType="KtonIpDynamicsChannelItem">
        update kton_ip_dynamics_channel_item
        <trim prefix="SET" suffixOverrides=",">
            <if test="merchantId != null">merchant_id = #{merchantId},</if>
            <if test="channelStatus != null">channel_status = #{channelStatus},</if>
            <if test="channelName != null">channel_name = #{channelName},</if>
            <if test="channelPassword != null">channel_password = #{channelPassword},</if>
            <if test="useTrafficGb != null">use_traffic_gb = #{useTrafficGb},</if>
            <if test="limitTrafficGb != null">limit_traffic_gb = #{limitTrafficGb},</if>
            <if test="remarks != null">remarks = #{remarks},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteKtonIpDynamicsChannelItemById" parameterType="Long">
        delete from kton_ip_dynamics_channel_item where id = #{id}
    </delete>

    <delete id="deleteKtonIpDynamicsChannelItemByIds" parameterType="String">
        delete from kton_ip_dynamics_channel_item where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>