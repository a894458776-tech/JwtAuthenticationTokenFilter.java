<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kylin.kton.system.mapper.KtonIpDynamicsConfigPeriodMapper">
    
    <resultMap type="KtonIpDynamicsConfigPeriod" id="KtonIpDynamicsConfigPeriodResult">
        <result property="id"    column="id"    />
        <result property="merchantId"    column="merchant_id"    />
        <result property="price"    column="price"    />
        <result property="currency"    column="currency"    />
        <result property="days"    column="days"    />
        <result property="dayDiscount"    column="day_discount"    />
        <result property="status"    column="status"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateTime"    column="update_time"    />
    </resultMap>

    <sql id="selectKtonIpDynamicsConfigPeriodVo">
        select id, merchant_id, price, currency, days, day_discount, status, create_time, update_time from kton_ip_dynamics_config_period
    </sql>

    <select id="selectKtonIpDynamicsConfigPeriodList" parameterType="KtonIpDynamicsConfigPeriod" resultMap="KtonIpDynamicsConfigPeriodResult">
        <include refid="selectKtonIpDynamicsConfigPeriodVo"/>
        <where>  
            <if test="merchantId != null "> and merchant_id = #{merchantId}</if>
            <if test="price != null "> and price = #{price}</if>
            <if test="currency != null  and currency != ''"> and currency = #{currency}</if>
            <if test="days != null "> and days = #{days}</if>
            <if test="dayDiscount != null "> and day_discount = #{dayDiscount}</if>
            <if test="status != null  and status != ''"> and status = #{status}</if>
        </where>
    </select>
    
    <select id="selectKtonIpDynamicsConfigPeriodById" parameterType="Long" resultMap="KtonIpDynamicsConfigPeriodResult">
        <include refid="selectKtonIpDynamicsConfigPeriodVo"/>
        where id = #{id}
    </select>

    <insert id="insertKtonIpDynamicsConfigPeriod" parameterType="KtonIpDynamicsConfigPeriod" useGeneratedKeys="true" keyProperty="id">
        insert into kton_ip_dynamics_config_period
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="merchantId != null">merchant_id,</if>
            <if test="price != null">price,</if>
            <if test="currency != null and currency != ''">currency,</if>
            <if test="days != null">days,</if>
            <if test="dayDiscount != null">day_discount,</if>
            <if test="status != null and status != ''">status,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateTime != null">update_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="merchantId != null">#{merchantId},</if>
            <if test="price != null">#{price},</if>
            <if test="currency != null and currency != ''">#{currency},</if>
            <if test="days != null">#{days},</if>
            <if test="dayDiscount != null">#{dayDiscount},</if>
            <if test="status != null and status != ''">#{status},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateTime != null">#{updateTime},</if>
         </trim>
    </insert>

    <update id="updateKtonIpDynamicsConfigPeriod" parameterType="KtonIpDynamicsConfigPeriod">
        update kton_ip_dynamics_config_period
        <trim prefix="SET" suffixOverrides=",">
            <if test="merchantId != null">merchant_id = #{merchantId},</if>
            <if test="price != null">price = #{price},</if>
            <if test="currency != null and currency != ''">currency = #{currency},</if>
            <if test="days != null">days = #{days},</if>
            <if test="dayDiscount != null">day_discount = #{dayDiscount},</if>
            <if test="status != null and status != ''">status = #{status},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteKtonIpDynamicsConfigPeriodById" parameterType="Long">
        delete from kton_ip_dynamics_config_period where id = #{id}
    </delete>

    <delete id="deleteKtonIpDynamicsConfigPeriodByIds" parameterType="String">
        delete from kton_ip_dynamics_config_period where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>