<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kylin.kton.system.mapper.KtonIpDynamicsConfigPlanMapper">
    
    <resultMap type="KtonIpDynamicsConfigPlan" id="KtonIpDynamicsConfigPlanResult">
        <result property="id"    column="id"    />
        <result property="merchantId"    column="merchant_id"    />
        <result property="title"    column="title"    />
        <result property="capacity"    column="capacity"    />
        <result property="price"    column="price"    />
        <result property="discount"    column="discount"    />
        <result property="status"    column="status"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateTime"    column="update_time"    />
    </resultMap>

    <sql id="selectKtonIpDynamicsConfigPlanVo">
        select id, merchant_id, title, capacity, price, discount, status, create_time, update_time from kton_ip_dynamics_config_plan
    </sql>

    <select id="selectKtonIpDynamicsConfigPlanList" parameterType="KtonIpDynamicsConfigPlan" resultMap="KtonIpDynamicsConfigPlanResult">
        <include refid="selectKtonIpDynamicsConfigPlanVo"/>
        <where>  
            <if test="merchantId != null "> and merchant_id = #{merchantId}</if>
            <if test="title != null  and title != ''"> and title = #{title}</if>
            <if test="capacity != null "> and capacity = #{capacity}</if>
            <if test="price != null "> and price = #{price}</if>
            <if test="discount != null "> and discount = #{discount}</if>
            <if test="status != null  and status != ''"> and status = #{status}</if>
        </where>
    </select>
    
    <select id="selectKtonIpDynamicsConfigPlanById" parameterType="Long" resultMap="KtonIpDynamicsConfigPlanResult">
        <include refid="selectKtonIpDynamicsConfigPlanVo"/>
        where id = #{id}
    </select>

    <insert id="insertKtonIpDynamicsConfigPlan" parameterType="KtonIpDynamicsConfigPlan" useGeneratedKeys="true" keyProperty="id">
        insert into kton_ip_dynamics_config_plan
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="merchantId != null">merchant_id,</if>
            <if test="title != null">title,</if>
            <if test="capacity != null">capacity,</if>
            <if test="price != null">price,</if>
            <if test="discount != null">discount,</if>
            <if test="status != null and status != ''">status,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateTime != null">update_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="merchantId != null">#{merchantId},</if>
            <if test="title != null">#{title},</if>
            <if test="capacity != null">#{capacity},</if>
            <if test="price != null">#{price},</if>
            <if test="discount != null">#{discount},</if>
            <if test="status != null and status != ''">#{status},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateTime != null">#{updateTime},</if>
         </trim>
    </insert>

    <update id="updateKtonIpDynamicsConfigPlan" parameterType="KtonIpDynamicsConfigPlan">
        update kton_ip_dynamics_config_plan
        <trim prefix="SET" suffixOverrides=",">
            <if test="merchantId != null">merchant_id = #{merchantId},</if>
            <if test="title != null">title = #{title},</if>
            <if test="capacity != null">capacity = #{capacity},</if>
            <if test="price != null">price = #{price},</if>
            <if test="discount != null">discount = #{discount},</if>
            <if test="status != null and status != ''">status = #{status},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteKtonIpDynamicsConfigPlanById" parameterType="Long">
        delete from kton_ip_dynamics_config_plan where id = #{id}
    </delete>

    <delete id="deleteKtonIpDynamicsConfigPlanByIds" parameterType="String">
        delete from kton_ip_dynamics_config_plan where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>