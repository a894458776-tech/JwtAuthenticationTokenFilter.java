<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kylin.kton.system.mapper.KtonIpDynamicsPlanMapper">
    
    <resultMap type="KtonIpDynamicsPlan" id="KtonIpDynamicsPlanResult">
        <result property="id"    column="id"    />
        <result property="merchantId"    column="merchant_id"    />
        <result property="planTrafficGb"    column="plan_traffic_gb"    />
        <result property="expirationDate"    column="expiration_date"    />
        <result property="expired"    column="expired"    />
        <result property="planResidueTrafficGb"    column="plan_residue_traffic_gb"    />
        <result property="currentPlanItemId"    column="current_plan_item_id"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateTime"    column="update_time"    />
    </resultMap>

    <sql id="selectKtonIpDynamicsPlanVo">
        select id, merchant_id, plan_traffic_gb, expiration_date, expired, plan_residue_traffic_gb, current_plan_item_id, create_time, update_time from kton_ip_dynamics_plan
    </sql>

    <select id="selectKtonIpDynamicsPlanList" parameterType="KtonIpDynamicsPlan" resultMap="KtonIpDynamicsPlanResult">
        <include refid="selectKtonIpDynamicsPlanVo"/>
        <where>  
            <if test="merchantId != null "> and merchant_id = #{merchantId}</if>
            <if test="planTrafficGb != null "> and plan_traffic_gb = #{planTrafficGb}</if>
            <if test="expirationDate != null "> and expiration_date = #{expirationDate}</if>
            <if test="expired != null "> and expired = #{expired}</if>
            <if test="planResidueTrafficGb != null "> and plan_residue_traffic_gb = #{planResidueTrafficGb}</if>
            <if test="currentPlanItemId != null "> and current_plan_item_id = #{currentPlanItemId}</if>
        </where>
    </select>
    
    <select id="selectKtonIpDynamicsPlanById" parameterType="Long" resultMap="KtonIpDynamicsPlanResult">
        <include refid="selectKtonIpDynamicsPlanVo"/>
        where id = #{id}
    </select>

    <insert id="insertKtonIpDynamicsPlan" parameterType="KtonIpDynamicsPlan">
        insert into kton_ip_dynamics_plan
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">id,</if>
            <if test="merchantId != null">merchant_id,</if>
            <if test="planTrafficGb != null">plan_traffic_gb,</if>
            <if test="expirationDate != null">expiration_date,</if>
            <if test="expired != null">expired,</if>
            <if test="planResidueTrafficGb != null">plan_residue_traffic_gb,</if>
            <if test="currentPlanItemId != null">current_plan_item_id,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateTime != null">update_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null">#{id},</if>
            <if test="merchantId != null">#{merchantId},</if>
            <if test="planTrafficGb != null">#{planTrafficGb},</if>
            <if test="expirationDate != null">#{expirationDate},</if>
            <if test="expired != null">#{expired},</if>
            <if test="planResidueTrafficGb != null">#{planResidueTrafficGb},</if>
            <if test="currentPlanItemId != null">#{currentPlanItemId},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateTime != null">#{updateTime},</if>
         </trim>
    </insert>

    <update id="updateKtonIpDynamicsPlan" parameterType="KtonIpDynamicsPlan">
        update kton_ip_dynamics_plan
        <trim prefix="SET" suffixOverrides=",">
            <if test="merchantId != null">merchant_id = #{merchantId},</if>
            <if test="planTrafficGb != null">plan_traffic_gb = #{planTrafficGb},</if>
            <if test="expirationDate != null">expiration_date = #{expirationDate},</if>
            <if test="expired != null">expired = #{expired},</if>
            <if test="planResidueTrafficGb != null">plan_residue_traffic_gb = #{planResidueTrafficGb},</if>
            <if test="currentPlanItemId != null">current_plan_item_id = #{currentPlanItemId},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteKtonIpDynamicsPlanById" parameterType="Long">
        delete from kton_ip_dynamics_plan where id = #{id}
    </delete>

    <delete id="deleteKtonIpDynamicsPlanByIds" parameterType="String">
        delete from kton_ip_dynamics_plan where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>