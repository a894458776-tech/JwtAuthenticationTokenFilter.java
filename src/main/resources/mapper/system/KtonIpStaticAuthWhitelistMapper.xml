<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kylin.kton.system.mapper.KtonIpStaticAuthWhitelistMapper">
    
    <resultMap type="KtonIpStaticAuthWhitelist" id="KtonIpStaticAuthWhitelistResult">
        <result property="id"    column="id"    />
        <result property="merchantId"    column="merchant_id"    />
        <result property="ipAddress"    column="ip_address"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateTime"    column="update_time"    />
    </resultMap>

    <sql id="selectKtonIpStaticAuthWhitelistVo">
        select id, merchant_id, ip_address, create_time, update_time from kton_ip_static_auth_whitelist
    </sql>

    <select id="selectKtonIpStaticAuthWhitelistList" parameterType="KtonIpStaticAuthWhitelist" resultMap="KtonIpStaticAuthWhitelistResult">
        <include refid="selectKtonIpStaticAuthWhitelistVo"/>
        <where>  
            <if test="merchantId != null "> and merchant_id = #{merchantId}</if>
            <if test="ipAddress != null  and ipAddress != ''"> and ip_address = #{ipAddress}</if>
        </where>
    </select>
    
    <select id="selectKtonIpStaticAuthWhitelistById" parameterType="Long" resultMap="KtonIpStaticAuthWhitelistResult">
        <include refid="selectKtonIpStaticAuthWhitelistVo"/>
        where id = #{id}
    </select>

    <insert id="insertKtonIpStaticAuthWhitelist" parameterType="KtonIpStaticAuthWhitelist">
        insert into kton_ip_static_auth_whitelist
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">id,</if>
            <if test="merchantId != null">merchant_id,</if>
            <if test="ipAddress != null and ipAddress != ''">ip_address,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateTime != null">update_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null">#{id},</if>
            <if test="merchantId != null">#{merchantId},</if>
            <if test="ipAddress != null and ipAddress != ''">#{ipAddress},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateTime != null">#{updateTime},</if>
         </trim>
    </insert>

    <update id="updateKtonIpStaticAuthWhitelist" parameterType="KtonIpStaticAuthWhitelist">
        update kton_ip_static_auth_whitelist
        <trim prefix="SET" suffixOverrides=",">
            <if test="merchantId != null">merchant_id = #{merchantId},</if>
            <if test="ipAddress != null and ipAddress != ''">ip_address = #{ipAddress},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteKtonIpStaticAuthWhitelistById" parameterType="Long">
        delete from kton_ip_static_auth_whitelist where id = #{id}
    </delete>

    <delete id="deleteKtonIpStaticAuthWhitelistByIds" parameterType="String">
        delete from kton_ip_static_auth_whitelist where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>