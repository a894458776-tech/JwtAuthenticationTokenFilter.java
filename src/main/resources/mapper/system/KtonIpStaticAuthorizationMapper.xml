<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kylin.kton.system.mapper.KtonIpStaticAuthorizationMapper">
    
    <resultMap type="KtonIpStaticAuthorization" id="KtonIpStaticAuthorizationResult">
        <result property="id"    column="id"    />
        <result property="merchantId"    column="merchant_id"    />
        <result property="userAuthorization"    column="user_authorization"    />
        <result property="ipAuthorization"    column="ip_authorization"    />
        <result property="authUsername"    column="auth_username"    />
        <result property="authPassword"    column="auth_password"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateTime"    column="update_time"    />
    </resultMap>

    <sql id="selectKtonIpStaticAuthorizationVo">
        select id, merchant_id, user_authorization, ip_authorization, auth_username, auth_password, create_time, update_time from kton_ip_static_authorization
    </sql>

    <select id="selectKtonIpStaticAuthorizationList" parameterType="KtonIpStaticAuthorization" resultMap="KtonIpStaticAuthorizationResult">
        <include refid="selectKtonIpStaticAuthorizationVo"/>
        <where>  
            <if test="merchantId != null "> and merchant_id = #{merchantId}</if>
            <if test="userAuthorization != null "> and user_authorization = #{userAuthorization}</if>
            <if test="ipAuthorization != null "> and ip_authorization = #{ipAuthorization}</if>
            <if test="authUsername != null  and authUsername != ''"> and auth_username like concat('%', #{authUsername}, '%')</if>
            <if test="authPassword != null  and authPassword != ''"> and auth_password = #{authPassword}</if>
        </where>
    </select>
    
    <select id="selectKtonIpStaticAuthorizationById" parameterType="Long" resultMap="KtonIpStaticAuthorizationResult">
        <include refid="selectKtonIpStaticAuthorizationVo"/>
        where id = #{id}
    </select>

    <insert id="insertKtonIpStaticAuthorization" parameterType="KtonIpStaticAuthorization">
        insert into kton_ip_static_authorization
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">id,</if>
            <if test="merchantId != null">merchant_id,</if>
            <if test="userAuthorization != null">user_authorization,</if>
            <if test="ipAuthorization != null">ip_authorization,</if>
            <if test="authUsername != null and authUsername != ''">auth_username,</if>
            <if test="authPassword != null and authPassword != ''">auth_password,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateTime != null">update_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null">#{id},</if>
            <if test="merchantId != null">#{merchantId},</if>
            <if test="userAuthorization != null">#{userAuthorization},</if>
            <if test="ipAuthorization != null">#{ipAuthorization},</if>
            <if test="authUsername != null and authUsername != ''">#{authUsername},</if>
            <if test="authPassword != null and authPassword != ''">#{authPassword},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateTime != null">#{updateTime},</if>
         </trim>
    </insert>

    <update id="updateKtonIpStaticAuthorization" parameterType="KtonIpStaticAuthorization">
        update kton_ip_static_authorization
        <trim prefix="SET" suffixOverrides=",">
            <if test="merchantId != null">merchant_id = #{merchantId},</if>
            <if test="userAuthorization != null">user_authorization = #{userAuthorization},</if>
            <if test="ipAuthorization != null">ip_authorization = #{ipAuthorization},</if>
            <if test="authUsername != null and authUsername != ''">auth_username = #{authUsername},</if>
            <if test="authPassword != null and authPassword != ''">auth_password = #{authPassword},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteKtonIpStaticAuthorizationById" parameterType="Long">
        delete from kton_ip_static_authorization where id = #{id}
    </delete>

    <delete id="deleteKtonIpStaticAuthorizationByIds" parameterType="String">
        delete from kton_ip_static_authorization where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>