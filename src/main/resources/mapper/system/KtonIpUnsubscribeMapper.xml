<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kylin.kton.system.mapper.KtonIpUnsubscribeMapper">
    
    <resultMap type="KtonIpUnsubscribe" id="KtonIpUnsubscribeResult">
        <result property="id"    column="id"    />
        <result property="merchantId"    column="merchant_id"    />
        <result property="ip"    column="ip"    />
        <result property="unsubscribeTime"    column="unsubscribe_time"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateTime"    column="update_time"    />
    </resultMap>

    <sql id="selectKtonIpUnsubscribeVo">
        select id, merchant_id, ip, unsubscribe_time, create_time, update_time from kton_ip_unsubscribe
    </sql>

    <select id="selectKtonIpUnsubscribeList" parameterType="KtonIpUnsubscribe" resultMap="KtonIpUnsubscribeResult">
        <include refid="selectKtonIpUnsubscribeVo"/>
        <where>  
            <if test="merchantId != null "> and merchant_id = #{merchantId}</if>
            <if test="ip != null  and ip != ''"> and ip = #{ip}</if>
            <if test="unsubscribeTime != null "> and unsubscribe_time = #{unsubscribeTime}</if>
        </where>
    </select>
    
    <select id="selectKtonIpUnsubscribeById" parameterType="Long" resultMap="KtonIpUnsubscribeResult">
        <include refid="selectKtonIpUnsubscribeVo"/>
        where id = #{id}
    </select>

    <insert id="insertKtonIpUnsubscribe" parameterType="KtonIpUnsubscribe">
        insert into kton_ip_unsubscribe
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">id,</if>
            <if test="merchantId != null">merchant_id,</if>
            <if test="ip != null">ip,</if>
            <if test="unsubscribeTime != null">unsubscribe_time,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateTime != null">update_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null">#{id},</if>
            <if test="merchantId != null">#{merchantId},</if>
            <if test="ip != null">#{ip},</if>
            <if test="unsubscribeTime != null">#{unsubscribeTime},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateTime != null">#{updateTime},</if>
         </trim>
    </insert>

    <update id="updateKtonIpUnsubscribe" parameterType="KtonIpUnsubscribe">
        update kton_ip_unsubscribe
        <trim prefix="SET" suffixOverrides=",">
            <if test="merchantId != null">merchant_id = #{merchantId},</if>
            <if test="ip != null">ip = #{ip},</if>
            <if test="unsubscribeTime != null">unsubscribe_time = #{unsubscribeTime},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteKtonIpUnsubscribeById" parameterType="Long">
        delete from kton_ip_unsubscribe where id = #{id}
    </delete>

    <delete id="deleteKtonIpUnsubscribeByIds" parameterType="String">
        delete from kton_ip_unsubscribe where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>